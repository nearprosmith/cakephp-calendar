{
    "name": "nearprosmitih/Calendar",
    "description": "Calendar plugin for CakePHP3",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Calendar\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Calendar\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
